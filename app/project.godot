Network="*res://autoload/network.gd"
io="*res://autoload/io.gd"
std="*res://autoload/std.gd"
Group="*res://autoload/group.gd"
Repo="*res://autoload/repo.gd"
Queue="*res://autoload/queue.tscn"
Controller="*res://autoload/controller.gd"
Finder="*res://autoload/finder.tscn"
Transition="*res://autoload/transition.tscn"
Secret="*res://autoload/secrets.gd"
Config="*res://autoload/config.gd"

[debug]

gdscript/warnings/integer_division=0

[display]

window/size/viewport_width=800
window/size/viewport_height=600
window/size/extend_to_title=true
window/stretch/mode="canvas_items"

[editor]

run/main_run_args="--campaign=maze --uri=localhost --port=5002 --network=1 --dir=maps/target --username=user1 --password=pass1 --log-level=trace"

[gui]

theme/custom_font="res://themes/UbuntuMono-Regular.ttf"
theme/default_font_hinting=0

